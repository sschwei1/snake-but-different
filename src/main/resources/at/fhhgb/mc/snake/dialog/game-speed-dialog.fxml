<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox
        xmlns="http://javafx.com/javafx/8.0.171"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="at.fhhgb.mc.snake.controller.dialog.GameSpeedDialogController"
        stylesheets="@../css/custom.css"
        styleClass="dialog-form"
>
    <Label
            text="Speed Settings"
            styleClass="header"
    />

    <Separator />

    <VBox>
        <Label fx:id="speedSliderLabel" text="Game Speed:" />
        <Slider fx:id="speedSlider" min="1" max="50" />
    </VBox>
</VBox>